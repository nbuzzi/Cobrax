<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5"/>
    </startup>

  <appSettings>
    <add key="HostAddress" value="localhost"/>
    <add key="PortNumber" value="100"/>
    <add key="UseProxy" value="false"/>
    <add key="RetryInterval" value="60000"/>
    
    <!--If there is no path, by default it will be windows/system32-->
    <add key="LoggerPath" value="logger.log"/>
    <add key="GhostMode" value="false"/>
  </appSettings>
</configuration>
